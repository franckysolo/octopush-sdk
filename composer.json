{
    "name": "franckysolo/octopush-sdk",
    "description": "Php SDK for octopush API SMS",
    "keywords": ["octopush", "php", "sms"],
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "franckysolo",
            "email": "franckysolo@gmail.com",
            "role": "Developer"
        }
    ],
    "autoload": {
        "classmap": ["src/"],
        "psr-4": {
            "Octopush\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
      "test": "./vendor/bin/phpunit",
      "docs": "@php ./vendor/bin/phpDocumentor.phar -d src -t docs"
    },
    "require": {
      "php": "^7.1.3"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.1"
    },
    "minimum-stability": "dev"
}
